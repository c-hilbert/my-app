{
  "name": "my-app",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "inbound": "node ./scripts/inbound-call.js",
    "outbound": "node ./scripts/outbound-call.js",
    "test": "jest",
    "dev": "nodemon app.js",
    "start": "node app.js",
    "lint": "eslint ."
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|my-project|react-native-button)/)"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageProvider": "v8",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  },
  "keywords": [],
  "author": "Charlie Weems",
  "license": "MIT",
  "dependencies": {
    "@deepgram/sdk": "^3.3.4",
    "@google/maps": "^1.1.3",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "mysql2": "^3.10.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "wavefile": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@flydotio/dockerfile": "^0.4.11",
    "babel-jest": "^29.7.0",
    "dotenv-safe": "^9.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.0.0"
  }
}
